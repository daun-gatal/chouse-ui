{
  "name": "chouseui",
  "private": true,
  "author": "daun-gatal",
  "version": "2.12.0",
  "license": "Apache-2.0",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "bun run --env-file=.env dev:all",
    "dev:web": "vite --host",
    "dev:server": "bun run --watch packages/server/src/index.ts",
    "dev:all": "bun run dev:server & bun run dev:web",
    "build": "bun run build:web && bun run build:server",
    "build:web": "vite build",
    "build:server": "cd packages/server && bun run build",
    "start": "bun run packages/server/src/index.ts",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "preview": "vite preview"
  },
  "dependencies": {
